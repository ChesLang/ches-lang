comment := "##" !"##" (.* "##") > "#" !new_line .*

spacing := " " > "\t"
id := [a-Z_] [0-9a-Z_]*

def_class := "class" spacing+ id new_line+

def_func := "@"? id spacing* "(" def_func_args? ")" spacing* new_line+
def_func_args := type spacing* id ("," spacing* type spacing* id)* ","?
